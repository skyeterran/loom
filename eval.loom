(let people_a (
    "Skye"
    "Sock"
    "Salad"
    "Lee"
    "Brandon"
    "Silvia"
))
(let person_a (random people_a))

(let people_b (
    "Zac"
    "Brennan"
    "Frank"
    "Jerma"
    "Rachel"
    "Prime"
))
(let person_b (random people_b))

(say person_a (format "My name is " person_a "!"))
(say person_b (format "And my name's " person_b "."))

(if (random (true false)) (do
    person_a: That's true!
    person_b: I guess so.
)(do
    person_a: That's not right.
    person_b: Yeah, for sure.
))
